# Copy this file to .env and fill in your details

# Session Cookie (get from browser after login)
SESSION_COOKIE=your_session_cookie_here

# CSRF Token (get from the page source or network requests)
CSRF_TOKEN=your_csrf_token_here

# Check interval in milliseconds (60000 = 1 minute)
CHECK_INTERVAL=60000

# Auto-allocate new leads (true/false)
AUTO_ALLOCATE=true

# User and Customer IDs (optional - will be extracted from session)
USER_ID=
CUSTOMER_ID=
